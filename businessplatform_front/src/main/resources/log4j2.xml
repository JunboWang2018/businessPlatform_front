<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="WARN">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <!--指定日期格式-->
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p [%t] %c %x - %m%n"/>
        </Console>
    </appenders>
    <loggers>
        <root level="all">
            <appender-ref ref="Console"/>
        </root>
    </loggers>

</Configuration>